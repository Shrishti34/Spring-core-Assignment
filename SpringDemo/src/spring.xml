<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<beans>

	<bean id = "triangle" class = "Demo.Triangle">
		<property name="point A" ref = "pointA" />
		<property name="point B" ref = "pointB" />
		<property name="point C" ref = "pointC" />
	</bean>



	<bean id = "pointA" class = "Demo.Point">
		<property name="x" value ="0"/>
		<property name="y" value ="0"/>
	</bean>
	
	<bean id = "pointB" class = "Demo.Point">
		<property name="x" value ="20"/>
		<property name="y" value ="0"/>
	</bean>
	
	<bean id = "pointC" class = "Demo.Point">
		<property name="x" value ="20"/>
		<property name="y" value ="0"/>
	</bean>
	
	<bean id = "Circle" class = "Demo.Circle">
		<property name="center" ref = "pointA" />
	
	</bean>
	
	
	<!--  <bean class = "org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcess">
	</bean>-->	
</beans>	
</beans>